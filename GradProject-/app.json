{
  "expo": {
    "name": "Qaeati",
    "slug": "GradProject",
    "version": "2.0.6",
    "orientation": "portrait",
    "icon": "./assets/Q.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Q2.png",
      "resizeMode": "contain",
      "backgroundColor": "#6A2B81"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
